  project deltavirus

  #~#  Deltaretrovirus basic skeleton project 

	# Create deltavirus project extension-specific modules 
	run file glue/core/projectSettings.glue

	# Create modules
	run file glue/core/deltaModules.glue

	# Define features 
	run file glue/core/deltaFeatures.glue

	# Import reference sequences from GenBank XML files 
	import source sources/ncbi-refseqs/

	# Create deltavirus references and attach feature locations
	run file glue/core/deltaMasterReferences.glue  

	# Import side data for reference sequences
	run file glue/core/deltaImportSideData.glue

	# Import alignments 
	run file glue/core/deltaAlignments.glue

	# Define constrained alignment tree
	run file glue/core/deltaAlignmentTree.glue

	# Record feature coverage in alignments
	run file glue/core/deltaRecordFeaturePresence.glue

	# Create phylogenies
	run file glue/core/deltaCreateRootPhylogenies.glue

	validate
	exit

